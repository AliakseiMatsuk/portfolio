mixin skills(title,obj)
			li.skills-list__item
				.skills-row
					.skills-row__title #{title}
					ul.skills-row__list
						each skill in obj
							li.skills-row__item
								.skill
									span.skill__title #{skill.skillTitle}
									svg.circles.skill__circles(viewbox='0 0 110 110')
										circle.circle__first(r='45', cx='50%', cy='50%', fill='none')
										circle.circle__second(class=`circle-${skill.skillPersent}` r='45', cx='50%', cy='50%', transform='rotate(-90 55 55)')

